    "name": "Image Gallery Viewer",
    "short_name": "Image Gallery Viewer",
    "description": "Image gallery viewer for images and videos",
    "author": "Abdul Khan",
    "version": "1",
    "offline_enabled": true,
    "background": {
        "service_worker": "scripts/background.js"
