{
   "manifest_version": 3,
   "name": "Image Viewer",
   "version": "2.0.5",
   "description": "Show all images from local folder in single page.",
   "permissions": [
      "bookmarks"
   ],
   "action": {
      "default_title": "Click to create bookmark"
   },
   "background": {
      "service_worker": "background.js"
   },
   "icons": {
      "48": "images/logo.png"
   },
   "options_ui": {
      "page": "options.html",
      "open_in_tab": true
   },
   "key": "",
   "web_accessible_resources": [
      {
         "resources": [
            "images/*",
            "script/*",
            "fonts/*"
         ],
         "matches": [
            "<all_urls>"
         ],
         "use_dynamic_url": false
      }
   ]
}