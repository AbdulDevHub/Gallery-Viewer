{
   "background": {
      "persistent": false,
      "scripts": [
         "background.js"
      ]
   },
   "browser_action": {
      "default_title": "Click to create bookmark"
   },
   "description": "Show all images from local folder in single page. スクロールだけでマンガを読む",
   "icons": {
      "48": "images/63x63.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Image Viewer",
   "options_page": "options.html",
   "permissions": [
      "bookmarks"
   ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.5",
   "web_accessible_resources": [
      "images/*",
      "script/*",
      "fonts/*"
   ]
}